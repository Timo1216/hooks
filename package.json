{
  "name": "hooks",
  "version": "1.0.1",
  "description": "react hooks",
  "main": "dist/index.js",
  "module": "es/index.js",
  "types": "es/index.d.js",
  "files": [
    "dist",
    "es",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "start": "dumi dev",
    "build": "npm-run-all clean build:js tsc",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist && rm -rf es",
    "build:js": "rollup -c",
    "tsc": "tsc --declarationDir ./es",
    "preversion": "npm run console",
    "version": "npm run release && git add -A",
    "postversion": "git push && git push --tags",
    "release": "node ./scripts/release",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "test": "umi-test",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "tnpm run lint -- --fix",
    "console": "node scripts/console"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.{js,ts}": [
      "prettier --parser=typescript --write",
      "git add ."
    ],
    "*.{less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.0",
    "@umijs/utils": "^3.5.20",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "dumi": "^1.1.38",
    "eslint": "^7.32.0",
    "husky": "^4.3.8",
    "jest": "^27.4.7",
    "lint-staged": "^10.5.4",
    "now": "^21.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rollup": "^2.62.0",
    "rollup-plugin-dts": "^4.1.0",
    "typescript": "^4.4.4",
    "umi-test": "^1.9.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Timo1216/hooks.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Timo1216/hooks/issues"
  },
  "homepage": "https://github.com/Timo1216/hooks#readme"
}
